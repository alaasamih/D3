    <div class="row">
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("left-nav-footer")
      </div>
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("middle-nav-footer")
      </div>
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("right-nav-footer")
      </div>
      <div class="hidden-xs hidden-sm col-md-3 col-lg-3">
