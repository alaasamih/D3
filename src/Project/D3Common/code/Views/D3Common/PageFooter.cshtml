@model Sitecore.Mvc.Presentation.RenderingModel
<div class="subfooter">
  <div class="container">
    <div class="row">
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("nav-footer-left")
      </div>
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("nav-footer-middle")
      </div>
      <div class="col-sm-4 col-md-3 col-lg-3">
        @Html.Sitecore().Placeholder("nav-footer-right")
      </div>
      <div class="hidden-xs hidden-sm col-md-3 col-lg-3">
        <a href="/" class="subfooter-logoLink" title="Dubai design district homepage">
          <span class='visually-hidden'>Dubai design district logo</span>
          <svg aria-hidden="true" role="presentation" class="">
            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="images/icons.svg#icon-logo"></use>
          </svg>
        </a>
      </div>
    </div>
  </div>
</div>
<div class="contact">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        @Html.Sitecore().Placeholder("contact-footer")
      </div>
    </div>
  </div>
</div>
<div class="social">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        @Html.Sitecore().Placeholder("social-footer")
      </div>
    </div>
  </div>
</div>
<div class="trademark">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        @Html.Sitecore().Placeholder("copyright-footer")
      </div>
    </div>
  </div>
</div>
